import = [
    "~/.config/alacritty/alacritty_schemes/Everforest Dark Medium.toml",
]


#[colors.bright]
#black = "#09070d"
#blue = "#ddcbfb"
#cyan = "#d6c2fa"
#green = "#aeb8f9"
#magenta = "#e17ef1"
#red = "#eb75a2"
#white = "#f7f6f9"
#yellow = "#c6cdfb"
#
#[colors.normal]
#black = "#1e1d20"
#blue = "#af88f2"
#cyan = "#8493f6"
#green = "#6577ec"
#magenta = "#d763e9"
#red = "#d53975"
#white = "#eae8ed"
#yellow = "#a0acf8"
#
#[colors.primary]
#background = "#1e1d20"
#foreground = "#98949e"

[font]
size = 14

[font.bold]
family = "JetBrainsMono Nerd Font"
style = "Bold"

[font.bold_italic]
family = "JetBrainsMono Nerd Font"
style = "Bold Italic"

[font.italic]
family = "JetBrainsMono Nerd Font"
style = "Italic"

[font.normal]
family = "JetBrainsMono Nerd Font"
style = "Medium"

[keyboard]
bindings = [
    { action = "ToggleFullscreen", key = "F11" },
]

[scrolling]
history = 50000

[window]
opacity = 0.8
